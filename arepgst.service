[Unit]
Description=GST Stream
After=network.target
StartLimitIntervalSec=0

[Service]
Type=simple
User=root
ExecStart=/usr/bin/gst-launch-1.0  v4l2src device=/dev/video0 ! "video/x-raw,width=640,height=480,framerate=30/1" ! videoconvert ! x264enc speed-preset=veryfast tune=zerolatency ! h264parse ! queue ! rtph264pay config-interval=1  pt=96 ! udpsink host=”IP_GCS” port=”UDP_PORT”
Restart=on-failure

[Install]
WantedBy=multi-user.target
